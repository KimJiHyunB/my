<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	
	<typeAliases>
		<typeAlias type="com.spring.exam2.vo.User" alias="user" />
		<typeAlias type="com.spring.exam2.vo.Product" alias="prod" />
		<typeAlias type="com.spring.exam2.vo.Cart" alias="cart" />
	</typeAliases>

	<mappers>
		<mapper resource="mapper/userMapper.xml" />
		<mapper resource="mapper/prodMapper.xml" />
		<mapper resource="mapper/cartMapper.xml" />
	</mappers>

</configuration>
